```scad
difference(){
    union(){
        cube([5,5,15]);
        translate([5,0,0])
            cube([5,5,5]);
    }
}

difference(){
    union(){
        translate([20,0,0]) cube([10,5,5]);
        translate([20,5,0]) cube([5,5,5]);
        translate([20,0,5]) cube([5,5,5]);
    }
}

difference(){
    union(){
        translate([40,0,0]) cube([10,5,5]);
        translate([40,5,0]) cube([5,5,5]);
        translate([40,0,5]) cube([5,5,5]);
    }
}


difference(){
    union(){
        translate([0,20,0]) cube([10,5,5]);
        translate([0,25,0]) cube([5,5,5]);
    }
}

difference(){
    union(){
        translate([0,40,0]) cube([10,5,5]);
        translate([5,45,0]) cube([10,5,5]);
    }
}

difference(){
    union(){
        translate([20,20,0]) cube([10,5,5]);
        translate([20,25,0]) cube([5,5,5]);
        translate([25,20,5]) cube([5,5,5]);
    }
}

difference(){
    union(){
        translate([45,25,0]) cube([10,5,5]);
        translate([45,30,0]) cube([5,5,5]);
        translate([40,25,0]) cube([5,5,5]);
    }
}
